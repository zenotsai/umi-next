{
  "name": "@example/lint",
  "scripts": {
    "lint": "umi lint",
    "lint:es": "umi lint '*.js' --eslint-only",
    "lint:es --fix": "umi lint '*.js' --eslint-only --fix",
    "lint:fix": "umi lint '*.js' --fix",
    "lint:style": "umi lint '*.less' --stylelint-only",
    "lint:style --fix": "umi lint '*.js' --stylelint-only --fix"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "stylelint": "^14.6.0",
    "umi": "4.0.0-canary.20220318.1"
  }
}
